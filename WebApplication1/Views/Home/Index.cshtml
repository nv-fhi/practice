@{
    ViewBag.Title = "Home Page";
}


<div id="tagsList" class="box">
    <div class="box-head">
        <h2 class="left">Tags</h2>
    </div>
    <div class="container">
        <input type="text" placeholder="Add New Tag" data-bind="value: tagToAdd, valueUpdate: 'OnKeyDown'" />
        <button class="btn-success" data-bind="click: addTag">+ Add</button>
        <ul data-bind="foreach: tags">
            <li class="tagItem" data-bind="click: $parent.selectTag">
                <span data-bind="text: Name"></span>
                <div>
                    <a href="#" class="tag-edit btn-warning">Edit</a>
                    <a href="#" class="tag-delete btn-danger">Delete</a>
                </div>
            </li>
        </ul>

    </div>l
</div>

<div id="tagDialog" data-bind="with : selectedTag">
    Tag Name:
    <input type="text" data-bind="value: Name" />
</div>

<div>
    <div>

    </div>



</div>


